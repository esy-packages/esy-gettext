{
  "name": "esy-gettext",
  "version": "0.20.000",
  "description": "gettext packaged for esy",
  "esy": {
    "buildsInSource": true,
    "exportedEnv": {
      "LDFLAGS": {
        "scope": "global",
        "val": "-L#{self.lib} $LDFLAGS"
      },
      "CFLAGS": {
        "scope": "global",
        "val": "-I#{self.install / 'include'} $CFLAGS"
      }
    },
    "build": [
      "find ./ -exec touch -t 200905010101 {} + ",
      [
        "bash",
        "-c",
        "#{os == 'windows' ? './configure --prefix=$cur__install --host x86_64-w64-mingw32' : './configure --prefix=$cur__install'}"
      ],
      [
        "make"
      ],
      [
        "make",
        "install"
      ]
    ]
  },
  "dependencies": {
    "esy-libiconv": "prometheansacrifice/esy-libiconv#38bab1ac2d53ae01b9c945620f6267446c4b2baa"
  }
}
